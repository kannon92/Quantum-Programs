
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import dfmp2

molecule {
0 1
F 
F 1 1.2
symmetry c1
}

set {
  basis 6-31g
  df_basis_mp2 cc-pvdz-ri
  freeze_core false
  scf_type df
  memory 3gb
}

set dfmp2 {
 print 1
 cholesky_tolerance 1e-3
}
energy('scf')
energy('mp2')
energy('conv-mp2')
energy('dfmp2')

dfmp2.exampleFN()

