
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import dfmp2

molecule {
0 1
H 
O 1 1.02
H 2 1.02 1 104.5 
symmetry c1
}

set {
  basis cc-pvdz
  df_basis_mp2 cc-pvdz-ri
  freeze_core false
  scf_type df
  memory 1gb
}

set dfmp2 {
 print 1
}
energy('scf')
energy('mp2')
energy('dfmp2')

dfmp2.exampleFN()

