
# PYTHONPATH must include directory above plugin directory.
#     Define either externally or here, then import plugin.
sys.path.insert(0, './..')
import dfmp2

molecule {
H 0.00   0.000    1.200
H 0.00   0.000    0.000
symmetry c1
}

set {
  basis cc-pvdz
  df_basis_mp2 cc-pvdz-ri
  freeze_core false
  memory 1gb
}

set dfmp2 {
  print 1
}
energy('scf')
energy('mp2')
energy('dfmp2')

dfmp2.exampleFN()

